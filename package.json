{
  "name": "type-usage",
  "version": "0.0.1",
  "description": "The usage text generator for type-args.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "test-cov": "jest --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangkaiyulw/type-usage.git"
  },
  "keywords": [
    "args",
    "options",
    "argument",
    "parser"
  ],
  "author": "Victor Zhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhangkaiyulw/type-usage/issues"
  },
  "homepage": "https://github.com/zhangkaiyulw/type-usage#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^0.9.1",
    "type-args": "^0.1.2"
  },
  "devDependencies": {
    "@types/node": "^12.6.8"
  }
}
